    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/x-charts": "^8.11.2",
    "@mui/x-date-pickers": "^8.12.0",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.8",
    "dayjs": "^1.11.18",
    "file-saver": "^2.0.5",
    "jwt-decode": "^4.0.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-number-format": "^5.4.4",
    "react-router-dom": "^7.8.2",
    "react-toastify": "^11.0.5",
    "remask": "^1.2.2",
